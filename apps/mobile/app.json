{
  "expo": {
    "name": "mobile",
    "slug": "mobile",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mobile",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "deploymentTarget": "13.4",
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.mobile",
      "infoPlist": {
        "UIBackgroundModes": ["audio"],
        "NSMicrophoneUsageDescription": "We use the microphone so your child can talk to the avatar.",
        "NSSpeechRecognitionUsageDescription": "We use speech recognition to understand your child's responses."
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.mobile",
      "permissions": ["RECORD_AUDIO"]
    },
    "web": {
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-sqlite"
    ],
    "extra": {
      "OPENAI_API_KEY": "${process.env.OPENAI_API_KEY}",
      "ELEVENLABS_API_KEY": "${process.env.ELEVENLABS_API_KEY}",
      "ELEVENLABS_VOICE_ID": "${process.env.ELEVENLABS_VOICE_ID}",
      "eas": {
        "projectId": "placeholder"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}